# Example environment variables for Acontext server (Go API + dependencies)
# Copy to .env and fill in values.

# Mode
APP_ENV=debug

# API
API_EXPORT_PORT=8029
ROOT_API_BEARER_TOKEN=replace-me

# Postgres
DATABASE_HOST=127.0.0.1
DATABASE_USER=acontext
DATABASE_PASSWORD=replace-me
DATABASE_NAME=acontext
DATABASE_EXPORT_PORT=15437
DATABASE_ENABLE_TLS=false

# Redis
REDIS_HOST=127.0.0.1
REDIS_EXPORT_PORT=16384
REDIS_PASSWORD=replace-me
REDIS_ENABLE_TLS=false

# RabbitMQ
RABBITMQ_HOST=127.0.0.1
RABBITMQ_USER=acontext
RABBITMQ_PASSWORD=replace-me
RABBITMQ_EXPORT_PORT=15682
RABBITMQ_VHOST_ENCODED=%2F

# S3 (optional)
S3_ENDPOINT=http://127.0.0.1:19005
S3_INTERNAL_ENDPOINT=http://127.0.0.1:19005
S3_REGION=us-east-1
S3_ACCESS_KEY=replace-me
S3_SECRET_KEY=replace-me
S3_BUCKET=acontext

# Core service
CORE_BASE_URL=http://127.0.0.1:8005

# OpenTelemetry (optional)
OTEL_EXPORTER_OTLP_ENDPOINT=
